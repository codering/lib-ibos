{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    ["module-resolver", {
      "root": ["./src/components"],
      "alias": {
        "antd": "./src/components",
        "ibos":"./src/components/ibos",
        "samples":"./samples"
      }
    }],
    ["import", { "libraryName": "antd", "libraryDirectory": "antd"}]
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    },
    "build":{
      "plugins": ["transform-remove-console"]
    }
  }
}